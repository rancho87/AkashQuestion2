
import java.util.*;

public class MyClass {
    
    public static void rotateClockwise(int[] arr ){
        
        int j = 0, temp;
        temp = arr[arr.length-1];
        for (int i = arr.length-1; i >0; i--) {
            arr[i]=arr[i-1];
        }
        arr[0]=temp;
    }
    

    public static void main(String args[]) {
      
       int [] arr ={1,2,3,4,5};
       MyClass.rotateClockwise(arr);
       for(int temp : arr)
         System.out.print(temp);
    }
    
    
}
